#!/usr/bin/make

create_addons_dir:
	make -C ${folder} $@

prebuild:
	make -C ${folder} $@

build:
	make -C ${folder} $@

test:
	make -C ${folder} $@

publish:
	make -C ${folder} $@

create_client_db:
	make -C ${folder} $@

run_unit_tests:
	make -C ${folder} $@

get_sql_dump:
	make -C ${folder} $@

run_sauce_connect:
	make -C ${folder} $@

run_server:
	make -C ${folder} $@

install_sauce_connect:
	make -C ${folder} $@

.PHONY: create_addons_dir prebuild build test publish create_client_db run_unit_tests get_sql_dump install_sauce_connect run_sauce_connect run_server
